# Search.
search.case.sensitive=no

# Reports.
column.padding=2
journal.info=off

# Date formatting.
dateformat=db
dateformat.annotation=Y-M-D
dateformat.report=Y-M-D
dateformat.edit=Y-M-D
dateformat.info=Y-M-D (a)

# Calendar.
weekstart=Monday
calendar.offset=on

# Colors.
color.project.none=white on red
color.uda.notes=yellow

